{
  "name": "python-getter-setter",
  "displayName": "Python Getter Setter",
  "description": "This extension generates python get and set methods from the class variable declarations.",
  "version": "1.1.7",
  "publisher": "Siddharth Verma",
  "repository": 
    {
        "type": "git",
        "url": "https://github.com/siddharthverma-1607/python-getter-setter-generator"    
    },
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "python-getter-setter.generatePythonGetterAndSetters"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "python-getter-setter.generatePythonGetterAndSetters",
      "title": "Generate Getter Setter for Python"
    }]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
